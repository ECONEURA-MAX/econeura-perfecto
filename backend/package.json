{
  "name": "@econeura/api-node",
  "version": "3.0.0",
  "description": "ECONEURA Backend API - Enterprise Grade with Prisma + NestJS ready",
  "main": "server.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "npm install --production --no-audit && node server.js",
    "postinstall": "echo 'Dependencies installed successfully'",
    "dev": "nodemon server.js",
    "build": "tsc || echo 'No TypeScript build yet'",
    "test": "jest --testPathPatterns=__tests__ --verbose",
    "test:watch": "jest --testPathPatterns=__tests__ --watch",
    "test:coverage": "jest --testPathPatterns=__tests__ --coverage",
    "test:e2e": "vitest run --config vitest.config.e2e.js",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/keyvault-secrets": "^4.10.0",
    "applicationinsights": "^3.3.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "@azure/storage-blob": "^12.24.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.73.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^2.1.0",
    "pg": "^8.13.1",
    "winston": "^3.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^8.57.1",
    "gitleaks": "^1.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "snyk": "^1.1300.1",
    "supertest": "^7.1.4",
    "typescript": "^5.4.0",
    "vitest": "^2.1.9"
  }
}
